<theme>

    <formatVersion>4</formatVersion>
    <include>./../gbz35.xml</include>

    <view name="system, basic, detailed, video">
		<image name="logo">
			<path>./system.svg</path>
		</image>
		<image name="background_pattern" extra="true">
			<path>./background.png</path>
			<origin>0.5 0.5</origin>
			<pos>0.5 0.5</pos>
			<size>1 1</size>
			<color>aeaeae</color>
			<tile>false</tile>
		</image>
		<textlist name="gamelist">
			<selectorColor>571C16</selectorColor>
			<!-- RED = 571C16, ORANGE = 562F0D, GREEN = 114D2A, 
			TURQUOISE = 0A473B, BLUE = 143952, PURPLE = 3A2144 -->
		</textlist>
	</view>
    <view name="basic"></view>
    <view name="detailed">
	
	</view>

</theme>
